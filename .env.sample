# Cloudflare Pages Deployment Configuration
# Copy this file to .env and fill in your actual values

# Cloudflare API Token
# Get your token from: https://dash.cloudflare.com/profile/api-tokens
# Required scopes: Cloudflare Pages:Edit
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token_here

# Cloudflare Pages Project Name
# This should match your project name in Cloudflare Pages dashboard
CLOUDFLARE_PAGES_PROJECT_NAME=ytomo-vial-kb-to-image

# Optional: Cloudflare Account ID
# CLOUDFLARE_ACCOUNT_ID=your_account_id_here
