{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run generate-mappings && npm run generate-styles && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "generate-mappings": "node scripts/generate-keyboard-mappings.cjs",
    "generate-styles": "node scripts/generate-styles.cjs",
    "deploy": "wrangler pages deploy dist",
    "deploy:prod": "npm run build && set -a && . ../.env && set +a && wrangler pages deploy dist --commit-dirty=true --commit-message='Deploy production build'"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@pinia/nuxt": "^0.11.2",
    "@types/uuid": "^10.0.0",
    "@uivjs/vue-markdown-preview": "^1.0.5",
    "jszip": "^3.10.1",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "uuid": "^13.0.0",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@pinia/testing": "^1.0.2",
    "@types/js-yaml": "^4.0.9",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "happy-dom": "^20.0.2",
    "js-yaml": "^4.1.0",
    "sass-embedded": "^1.92.1",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  }
}
